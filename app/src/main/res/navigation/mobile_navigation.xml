<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.savorly.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_savedlist"
        android:name="com.example.savorly.ui.savedlist.SavedlistFragment"
        android:label="Saved List"
        tools:layout="@layout/fragment_savedlist" />

    <fragment
        android:id="@+id/navigation_addpic"
        android:name="com.example.savorly.ui.addpic.AddpicFragment"
        android:label="Add Post"
        tools:layout="@layout/fragment_addpic" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.savorly.ui.profile.ProfileFragment"
        android:label="Profile"
        android:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_feed"
        android:name="com.example.savorly.ui.feed.FeedFragment"
        android:label="Feed"
        android:layout="@layout/fragment_feed" />
</navigation>